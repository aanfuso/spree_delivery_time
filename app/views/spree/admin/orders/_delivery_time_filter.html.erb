<div class="time-range-filter field">
  <%= label_tag nil, t(:start_delivery_time) %>
  <div class="time-range-fields">
    <%= f.text_field :start_delivery_time_gt, class: 'datepicker datepicker-from', value: params[:q][:start_delivery_time_gt], placeholder: t(:start) %>

    <span class="range-divider">
      <i class="icon-arrow-right"></i>
    </span>

    <%= f.text_field :start_delivery_time_lt, class: 'datepicker datepicker-to', value: params[:q][:start_delivery_time_lt], placeholder: t(:stop) %>
  </div>
</div>
